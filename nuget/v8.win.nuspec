<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>BaristaLabs.Espresso.v8.win-$Platform$</id>
    <version>$Version$</version>
    <authors>The V8 JavaScript Engine Authors</authors>
    <owners>The V8 NuGet package Authors</owners>
    <projectUrl>https://github.com/BaristaLabs/Espresso-v8-win</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/wiki/v8/v8/images/v8logo.png</iconUrl>
    <license type="expression">BSD-3-Clause</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Developer files (headers/libraries) for V8 JavaScript Engine.</description>
    <tags>V8 javascript embedded native nativepackage</tags>
    <dependencies>
      <dependency id="v8.redist-$PlatformToolset$-$Platform$" version="[$Version$]" /> 
    </dependencies>
  </metadata>
  <files>
    <file src="..\v8\include\**\*.h" target="include" />
    <file src="..\v8\out.gn\$BuildVersion$\$PlatformToolset$\$Platform$\Release\v8*.lib" target="lib\Release" />
    <file src="..\v8\out.gn\$BuildVersion$\$PlatformToolset$\$Platform$\Release\v8*.exp" target="lib\Release" />
    <file src="..\v8\out.gn\$BuildVersion$\$PlatformToolset$\$Platform$\Release\v8_build_config.json" target="lib\Release" />
    <file src="v8-$PlatformToolset$-$Platform$.props" target="build\native" />
    <file src="..\readme.md" target="" />
  </files>
</package>
