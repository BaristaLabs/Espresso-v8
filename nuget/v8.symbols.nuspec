<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>BaristaLabs.Espresso.v8-symbols-win-$Platform$</id>
    <version>$Version$</version>
    <authors>The V8 JavaScript Engine Authors</authors>
    <owners>The V8 NuGet package Authors</owners>
    <projectUrl>https://github.com/BaristaLabs/Espresso-v8-win</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/wiki/v8/v8/images/v8logo.png</iconUrl>
    <licenseUrl>https://raw.githubusercontent.com/v8/v8/master/LICENSE.v8</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Debug symbols for V8 JavaScript Engine redistrubutable files.</description>
    <tags>V8 javascript embedded native nativepackage</tags>
    <dependencies>
      <dependency id="BaristaLabs.Espresso.v8-redist-win-$Platform$" version="[$version$]" /> 
    </dependencies>
  </metadata>
  <files>
    <file src="..\v8\out.gn\$BuildVersion$\$PlatformToolset$\$Platform$\Debug\v8*.pdb" target="lib\Debug" />
    <file src="..\v8\out.gn\$BuildVersion$\$PlatformToolset$\$Platform$\Release\v8*.pdb" target="lib\Release" />
    <file src="v8.symbols-$PlatformToolset$-$Platform$.props" target="build\native" />
    <file src="..\readme.md" target="" />
  </files>
</package>
