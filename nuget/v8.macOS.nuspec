<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>BaristaLabs.Espresso.v8.macOS-$Configuration$</id>
    <version>$Version$</version>
    <authors>The V8 JavaScript Engine Authors</authors>
    <owners>The V8 NuGet package Authors</owners>
    <projectUrl>https://github.com/BaristaLabs/Espresso-v8</projectUrl>
    <iconUrl>https://github.com/BaristaLabs/BaristaCore/raw/master/Logo.png</iconUrl>
    <license type="expression">BSD-3-Clause</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Developer files (headers/libraries) for V8 JavaScript Engine.</description>
    <tags>V8 javascript embedded native nativepackage</tags>
    <dependencies>
      <dependency id="BaristaLabs.Espresso.v8.osx-redist-$Configuration$" version="[$Version$]" /> 
    </dependencies>
  </metadata>
  <files>
    <file src=".\v8\v8\include\**\*.h" target="include" />
    <file src=".\v8\v8\out.gn\$Configuration$\libv8*.toc" target="lib\$Configuration$\" />
    <file src=".\v8\v8\out.gn\$Configuration$\libicu*.toc" target="lib\$Configuration$\" />
    <file src=".\v8\v8\out.gn\$Configuration$\v8_build_config.json" target="lib\$Configuration$\" />
    <file src=".\readme.md" target="" />
  </files>
</package>
